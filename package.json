{
  "name": "image-viewer-gallery",
  "version": "3.0.0",
  "description": "A lib for image display.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "sideEffects": [
    "dist/*",
    "*.less"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "dev": "webpack-dev-server --config build/webpack.config.dev.js",
    "build:script": "npm run type-check && npm run lint && gulp -f ./build/build-script.js",
    "build:style": "npm run stylelint && gulp -f ./build/build-style.js",
    "build": "npm run build:script && npm run build:style",
    "lint": "eslint --ext .ts src",
    "stylelint": "stylelint src/**/*.less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyLeoHC/ImageViewer.git"
  },
  "keywords": [
    "image"
  ],
  "author": "KyLeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyLeoHC/ImageViewer/issues"
  },
  "homepage": "https://github.com/KyLeoHC/ImageViewer#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "autoprefixer": "^9.7.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.4.2",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-typescript": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "inline-source-webpack-plugin": "^1.4.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.3",
    "merge2": "^1.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-load-config": "^2.1.0",
    "postcss-loader": "^3.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.1.0",
    "through2": "^3.0.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/runtime": "7.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "git add"
    ],
    "src/**/*.less": [
      "npm run stylelint",
      "git add"
    ]
  }
}
